<h6>form render:new e edit <h6>


<%if @carcategory.errors.full_messages.any? %>
  <p>Não foi possível salvar, verifique os erros abaixo: </p>
  <%@carcategory.errors.full_messages.each do |erro| %>
    <p> <%= erro %> </p>
  <% end %>
  <% end %>  



<%= form_for( @carcategory , local: true) do |f| %>
  <%= f.label :name %>
  <%= f.text_field(:name) %>
  <%= f.label :dialyrate %>
  <%= f.number_field :dialyrate %>
  <%= f.label :insurance %>
  <%= f.number_field :insurance %>
  <%= f.label :thirdinsurance %>
  <%= f.number_field :thirdinsurance%>
  <%= f.submit('Enviar') %>
<% end %>