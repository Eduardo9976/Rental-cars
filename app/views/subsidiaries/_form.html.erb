<

<%if @subsidiary.errors.full_messages.any? %>
  <p>Não foi possível salvar, verifique os erros abaixo: </p>
  <% @subsidiary.errors.full_messages.each do |erro|%>
    <p> <%= erro %>  </p>
  <% end %>
  <% end %>


<%= form_for( @subsidiary , local: true) do |f| %>
  <%= f.label :name, 'Nome:'%>
  <%= f.text_field (:name) %>
  <%= f.label :cnpj, 'CNPJ:'%>
  <%= f.text_field (:cnpj) %>
  <%= f.label :address, 'Endereço:'%>
  <%= f.text_field (:address) %>
  <%= f.submit('Enviar') %>
<% end %>